{
  "name": "nodebb-plugin-bonc-sso",
  "version": "4.6.3",
  "description": "Allows login sessions from your app to persist in NodeBB",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FaustYKKN/nodebb-plugin-session-sharing"
  },
  "keywords": [
    "nodebb",
    "plugin"
  ],
  "author": {
    "name": "kknnpm",
    "email": "lingyingcome@163.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FaustYKKN/nodebb-plugin-session-sharing/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.6.2 || ^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^2",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^7.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0"
  }
}
